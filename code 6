#include <stdio.h>

int main() 
{
    int a, b, c;
    
    
    
    printf("Enter 3 numbers: \n");
    scanf("%d %d %d",&a, &b, &c);
    
    if(a>b && a<c){
    printf("a is greatest...%d",a);}
    else if(b>a && b>c){
    printf("b is greatest...%d",b);}
    else{
    printf("c is greatest...%c",c);
    }
    return 0;
}
