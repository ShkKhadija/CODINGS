#include<stdio.h>
 
 int main() 
 {
     int a, b;
     printf("Enter First number:");
     scanf("%d", a);
    
     printf("Enter second number");
     scanf ("%d", b);
     b=b-a;
     a=b+a;
     b=a-b;
     
     printf("New value of First number:%d",a);
     printf("New value of second number:%d",b);
     return 0;
 }
