#include <stdio.h>
float per(int s1, int s2, int s3, int s4, int s5) {
    return ( s1 + s2 + s3 +s4 + s5 )/5;
    
}
int main() {
    int a, b, c, d, e;
    printf("enetr marks of subjects");
    scanf("%d %d %d %d %d",&a, &b, &c, &d, &e);
    float result = per(a, b, c, d, e);
    printf("result is: %f", result);
   


    return 0;
}
